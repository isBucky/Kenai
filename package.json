{
    "name": "kenai",
    "version": "1.0.7",
    "description": "Creation and management of Fastify routes using decorators.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist/**/*",
        "*.js",
        "*.d.ts",
        "*.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "default": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./params": {
            "default": "./dist/src/decorators/params/index.js",
            "types": "./dist/src/decorators/params/index.d.ts"
        },
        "./methods": {
            "default": "./dist/src/decorators/methods/index.js",
            "types": "./dist/src/decorators/methods/index.d.ts"
        }
    },
    "scripts": {
        "build": "rm -rf dist && tsc && tsc-alias -p ./tsconfig.json",
        "publish": "bun build && npm publish"
    },
    "keywords": [],
    "author": "Bucky <bucky.developer@gmail.com>",
    "license": "MIT",
    "peerDependencies": {
        "reflect-metadata": "*"
    },
    "dependencies": {
        "@duplojs/zod-accelerator": "^2.4.0",
        "bucky.js": "^1.2.1",
        "fastify-plugin": "^5.0.1",
        "ioredis": "^5.4.1",
        "object.mn": "^1.2.6",
        "zod": "^3.23.8",
        "zod-openapi": "^3.1.1"
    },
    "devDependencies": {
        "@fastify/swagger": "^9.2.0",
        "@types/eslint-plugin-prettier": "^3.1.3",
        "@types/node": "^20.16.11",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "fastify": "^4.28.1",
        "prettier": "^3.3.3",
        "reflect-metadata": "^0.2.2",
        "typescript": "^5.6.3"
    }
}
